
public class Car {

	String color;	// This field has a default value of null
	String make;	// This field has a default value of null
	int numberOfWheels = 4; //This field is instantiated and has value of 4
							// otherwise it would have had value of 0
	boolean isSUV; 	// This field has a default value of false
	
	public Car() {

	}
	

}
